{
    "name": "Canvas Spring Java Sample",
    "description": "A simple salesforce to heroku canvas app",
    "website": "https://github.com/jthurst01/canvas-app-json",
    "success_url": "/welcome.jsp",
    "env": {
        "FIRST_NAME": {
            "description": "So that we can be personal",
            "default": "John",
            "required": true
        },
        "LAST_NAME": {
            "description": "Has a default but can be overridden",
            "default": "Doe"
        },
        "INSTALLED_BY": {
            "description": "Who installed this for you",
            "default": "Unknown"
        },
        "COOKIE_SECRET": {
            "description": "This gets generated",
            "generator": "secret"
        }
    }
}